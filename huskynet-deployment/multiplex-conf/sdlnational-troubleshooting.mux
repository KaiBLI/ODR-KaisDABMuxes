general {
	dabmode 1
}
ensemble {
	id 0x4fff
	ecc 0xec ; Extended Country Code	
	local-time-offset auto
	international-table 1
	label "SDL National"
	shortlabel "SDL"
}
services {
	srv-abs80 { label "Absolute 80s" }
	srv-abs90 { label "Absolute 90s" }
	srv-abscr { label "Absolute CR"}
	srv-absc { label "Absolute Ctry" }
	srv-boom { label "Boom Radio UK" }
	srv-bfbs { label "BFBS UK" }
	srv-fix { label "Fix Radio" }
	srv-jazz { label "Jazz FM" }
	srv-mcls { label "Magic Clss" }
	srv-msol { label "Magic Soul" }
	srv-mmgc { label "Mellow Magic" }
	srv-kerr { label "Kerrang!" }
	srv-virg { label "Virgin Radio" }
	srv-time { label "Times Radio" }
}
subchannels {
	srv-abs80-sub1 {
		type audio
		bitrate 128
		id 1
		protection 5
		inputproto edi
		inputuri "tcp://*:11001"
	}
	srv-abs90-sub1 {
		type audio
		bitrate 96
		id 2
		protection 5
		inputproto edi
		inputuri "tcp://*:11002"
	}
