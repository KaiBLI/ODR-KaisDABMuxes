	comp-abs80 {
		service srv-abs80
		subchannel srv-abs80-sub1
	}
	comp-abs90 {
		service srv-abs90
		subchannel srv-abs90-sub1
	}
	comp-abscr {
		service srv-abscr
		subchannel srv-abscr-sub1
	}
	comp-absc {
		service srv-absc
		subchannel srv-absc-sub1
	}
	comp-boom {
